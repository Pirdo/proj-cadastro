<!DOCTYPE html>
<html>
  <head>
    <title>Lista de Produto</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= mensagem %></h1>
    <ul>
      <% docs.forEach(function(cliente){ %>
        <li>
            <%= cliente.nome %> - <%= cliente.sobrenome %>
            <input type="button" onclick="location.href='/delete/cliente/<%= cliente.id %>'" 
             value="delete" />
            <input type="button" onclick="location.href='/edit/cliente/<%= cliente.id %>'" 
             value="editar" />
        </li>
      <% }) %>
    </ul>
    <br>
    <br>
    <input type="button" onclick="location.href='/cliente/cadastro'"  value="Novo Produto" />  
  </body>
</html>